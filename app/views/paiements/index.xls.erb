<table border="0">
  <tr>
    <th>ID</th>
	<th>Date</th>
	<th>Famille</th>
	<th>Ref</th>
	<th>Type Paiement</th>
	<th>Banque</th>
	<th>Cheque Num</th>
	<th>Montant Cantine</th>
	<th>Montant Garderie</th>
	<th>Montant</th>
	<th>Date de remise</th>
	<th>Memo</th>
	<th>Facture id</th>
	<th>Created_at</th>
	<th>Updated_at</th>
  </tr>
  <% @paiements.each do |p| %>
  <tr>
    <td><%= p.id %></td>
    <td><%= p.date %></td>
    <td><%= Famille.find(p.famille).nom.upcase %></td>
    <td><%= p.ref %></td>
    <td><%= p.typepaiement %></td>
    <td><%= p.banque %></td>
	<td><%= p.chequenum %></td>
	<td><%= p.montantCantine %></td>
	<td><%= p.montantGarderie %></td>
    <td><%= p.montant %></td>
	<td><%= p.remise %></td>
	<td><%= p.memo %></td>
	<td><%= p.facture_id %></td>
    <td><%= p.created_at %></td>
    <td><%= p.updated_at %></td>
  </tr>
  <% end %>
</table>


